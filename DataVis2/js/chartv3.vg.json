{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/mvitas1/3179/main/DataVis2/data/world-data-2023.csv"},
    "transform": [{
      "filter": {"and": [
        {"field": "Minimum wage", "valid": true},
        {"field": "Unemployment rate", "valid": true}
      ]}
    }],
    "mark": "rect",
    "width": 300,
    "height": 200,
    "encoding": {
      "x": {
        "bin": {"maxbins":20},
        "field": "Minimum wage",
        "type": "quantitative"
      },
      "y": {
        "bin": {"maxbins": 30},
        "field": "Unemployment rate",
        "type": "quantitative"
      },
      "color": {
        "aggregate": "count",
        "type": "quantitative"
      }
    },
    "config": {
      "view": {
        "stroke": "transparent"
      }
    }
  }