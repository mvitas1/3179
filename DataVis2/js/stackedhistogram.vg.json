{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/mvitas1/3179/main/DataVis2/data/world-data-2023.csv",
      "format": {
        "type": "csv",
        "parse": {"Life expectancy": "number"}
      }
    },
    "mark": "bar",
    "encoding": {
      "x": {
        "bin": {"maxbins": 20},
        "field": "Life expectancy",
        "type": "quantitative",
        "title": "Life Expectancy Bins"
      },
      "y": {
        "aggregate": "count",
        "type": "quantitative",
        "title": "Count"
      },
      "color": {
        "field": "Continent",
        "type": "nominal",
        "scale": {
          "domain": ["Africa", "Asia", "Europe", "North America", "Australia/Oceania", "South America"],
          "range": ["#1f77b4", "#ff7f0e", "#2ca02c", "#d62728", "#9467bd", "#8c564b"]
        },
        "title": "Continent"
      }
    }
  }
  
  
  
  
  
  